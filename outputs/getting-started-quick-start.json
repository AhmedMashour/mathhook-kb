      "post": {
        "description": "Get up and running with MathHook in 5 minutes. Learn basic expression creation,\nparsing, differentiation, and common operations across Rust, Python, and Node.js.\n",
        "requestBody": {
          "content": {
            "application/json": {
              "examples": {
                "computing_derivatives": {
                  "description": "Compute first and second derivatives of x^3",
                  "summary": "Computing Derivatives",
                  "value": {
                    "expression": "Expression.symbol('x')"
                  }
                },
                "first_expression_-_quadratic": {
                  "description": "Build and simplify x^2 + 2x + 1",
                  "summary": "First Expression - Quadratic",
                  "value": {
                    "expression": "Expression.symbol('x')"
                  }
                },
                "parsing_latex": {
                  "description": "Parse LaTeX notation into symbolic expression",
                  "summary": "Parsing LaTeX",
                  "value": {
                    "expression": "Parser(ParserConfig.default())"
                  }
                }
              },
              "schema": {
                "$ref": "#/components/schemas/Request"
              }
            }
          },
          "required": true
        },
